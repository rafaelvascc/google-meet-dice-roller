{
    "name": "Google Meet Dice Roller",
    "version": "0.0.0",
    "description": "Dice roller extension for RPG players using Google Meet",
    "permissions": [
      "storage",
      "tabs"
    ],
    "manifest_version": 2,
    "icons": {
        "16": "icons/64.png",
        "32": "icons/64.png",
        "48": "icons/64.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_icon": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": []
        }
    ]
}